<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>ASP.NET Core MVC implementation.</Description>
    <Copyright>Picums 2016 - 2017</Copyright>
    <AssemblyTitle>MVC</AssemblyTitle>
    <Version>0.31.0</Version>
    <WarningsAsErrors>true</WarningsAsErrors>
    <AssemblyName>Lizards.MvcToolkit.Core</AssemblyName>
  </PropertyGroup>
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>Lizards.MvcToolkit.Core</RootNamespace>
